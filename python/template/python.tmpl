import ctypes
import os
import sys
import traceback
# setup sys.excepthook
def excepthook(type, value, tb):
    sys.stderr.write("".join(traceback.format_exception(type, value, tb)))
    sys.stderr.flush()
    sys.exit(-1)

sys.excepthook = excepthook

lib = ctypes.CDLL("/bin/seccomp.so")
lib.LoadSeccomp.argtypes = []
lib.LoadSeccomp.restype = None

lib.LoadSeccomp()

if __name__ == "__main__":
    {{.Indent 4 .Code}}
    main()